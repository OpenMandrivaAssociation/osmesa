+   if (ctx->kcpu_init)
+      return true;
+
+   struct kbase_ioctl_kcpu_queue_new create = {0};
+
+   int ret;
+   ret = ioctl(k->fd, KBASE_IOCTL_KCPU_QUEUE_CREATE, &create);
