+   while (i < k->gpuprops_size) {
+      x = 0;
+      memcpy(&x, k->gpuprops + i, 4);
+      i += 4;
+
+      int size = 1 << (x & 3);
+      int this_name = x >> 2;
+
+      x = 0;
+      memcpy(&x, k->gpuprops + i, size);
+      i += size;
+
+      if (this_name == name) {
+         *value = x;
+         return true;
+      }
+   }
+
