+static uint64_t
+pan_get_gpuprop(struct state *s, int name)
+{
+   int i = 0;
+   uint64_t x = 0;
+   while (i < s->gpuprops_size) {
+      x = 0;
+      memcpy(&x, s->gpuprops + i, 4);
+      i += 4;
+
+      int size = 1 << (x & 3);
+      int this_name = x >> 2;
+
+      x = 0;
+      memcpy(&x, s->gpuprops + i, size);
+      i += size;
+
+      if (this_name == name)
+         return x;
+   }
+
+   fprintf(stderr, "Unknown prop %i\n", name);
+   return 0;
+}
