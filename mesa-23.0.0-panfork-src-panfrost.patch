+      uint64_t v1 = st_64[i * 2];
+      uint64_t v2 = st_64[i * 2 + 1];
+
+      if (!v1 && !v2)
+         continue;
+
+      pandecode_log("0x%2x: 0x%16" PRIx64 " 0x%16" PRIx64 "\n", i * 4, v1, v2);
+   }
