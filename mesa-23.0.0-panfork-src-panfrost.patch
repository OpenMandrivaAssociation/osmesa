+      [7] = "G610",      [16 + 2] = "G715", /* TODO: Immortalis instead of Mali?
+                                             */
+      [16 + 3] = "G615",
+   };
+   const char *name = (prod < ARRAY_SIZE(names)) ? names[prod] : NULL;
+   if (!name)
+      name = "unknown";
+
+   if (pr)
+      printf("v%i.%i.%i Mali-%s (%i): ", maj, min, rev, name, product);
+
+   if (maj < 10) {
+      printf("not v10 or later: ");
