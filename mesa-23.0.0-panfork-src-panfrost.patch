+#if PAN_BASE_API < 2
+static uint8_t
+kbase_latest_slot(uint8_t a, uint8_t b, uint8_t newest)
+{
+   /* If a == 4 and newest == 5, a will become 255 */
+   a -= newest;
+   b -= newest;
+   a = MAX2(a, b);
+   a += newest;
+   return a;
+}
